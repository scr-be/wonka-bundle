{
  "name": "src-run/wonka-bundle",
  "license": "MIT",
  "type": "project",
  "homepage": "https://src.run/wonka-bundle",
  "description": "a collection of classes and interfaces aimed at facilitating Symfony bundle's, including helpers for advanced bundle configuration, kernel compiler passes, and more",
  "support": {
    "email": "rmf@src.run",
    "issues": "https://src.run/wonka-bundle/issues",
    "source": "https://src.run/wonka-bundle/git"
  },
  "keywords": [
    "wonka-bundle", "wonka", "bundle", "src-run", "rmf"
  ],
  "authors": [
    {
      "name": "Rob Frawley 2nd",
      "email": "rmf@src.run",
      "homepage": "https://src.run/rmf",
      "role": "Project Lead"
    }
  ],
  "require": {
    "php": "~7.0",
    "src-run/wonka-library": "~0.7",
    "symfony/config": "~3.0",
    "symfony/http-foundation": "~3.0",
    "symfony/dependency-injection": "~3.0",
    "twig/twig": "~1.0 || ~2.0@dev"
  },
  "require-dev": {
    "codacy/coverage": "~1.0",
    "friendsofphp/php-cs-fixer": "v2.0.0-alpha",
    "phpunit/phpunit": "~5.4",
    "psr/log": "~1.0",
    "satooshi/php-coveralls": "~1.0",
    "sllh/php-cs-fixer-styleci-bridge": "~2.1",
    "symfony/symfony": "~3.0",
    "symfony/framework-bundle": "~3.0",
    "symfony/debug-bundle": "~3.0",
    "symfony/twig-bundle": "~3.0",
    "symfony/stopwatch": "~3.0",
    "symfony/phpunit-bridge": "~3.0",
    "symfony/var-dumper": "~3.0",
    "symfony/monolog-bundle": "~2.11 || ~3.0"
  },
  "autoload": {
    "psr-4": {
      "SR\\WonkaBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SR\\WonkaBundle\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.7-dev"
    }
  }
}
