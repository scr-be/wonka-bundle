{
    "name"        : "scribe/wonka-bundle",
    "license"     : "MIT",
    "type"        : "project",
    "homepage"    : "https://scr.be/wonka-bundle",
    "description" : "A collection of objects/interfaces to simplify bundle setup, such as advanced bundle configuration, compiler passes, and deprecaton error handlers.",
    "support" : {
        "email"  : "rmf@scr.be",
        "issues" : "https://wonka-bundle.oss.scr.be/issues",
        "source" : "https://wonka-bundle.oss.scr.be/git",
        "irc"    : "irc://irc.efnet.org/scribe"
    },
    "keywords"   : [
        "wonka-bundle", "scribe-wonka-bundle", "bundle", "scr.be", "scribe", "scribenet", "scribe-software", "scribe-systems"
    ],
    "authors"    : [
        {
            "name"    : "Rob Frawley 2nd",
            "email"   : "rmf@scr.be",
            "homepage": "https://scr.be/rmf",
            "role"    : "Project Lead"
        }
    ],
    "require" : {
        "php"                  : ">=5.6.4,~5.6|~7.0",
        "scribe/wonka-library" : "~0.1@dev"
    },
    "require-dev" : {
        "symfony/symfony"                      : ">=2.7",
        "symfony/framework-bundle"             : ">=2.7",
        "symfony/yaml"                         : ">=2.7",
        "symfony/filesystem"                   : ">=2.7",
        "symfony/finder"                       : ">=2.7",
        "symfony/dependency-injection"         : ">=2.7",
        "symfony/monolog-bundle"               : ">=2.7",
        "symfony/stopwatch"                    : ">=2.7",
        "symfony/var-dumper"                   : ">=2.7",
        "symfony/debug-bundle"                 : ">=2.7",
        "symfony/phpunit-bridge"               : ">=2.7",
        "phpunit/phpunit"                      : "~4.8",
        "sensio/distribution-bundle"           : "~4.0",
        "sensio/framework-extra-bundle"        : "~3.0",
        "sensio/generator-bundle"              : "~2.5",
        "doctrine/doctrine-fixtures-bundle"    : "~2.0|~3.0",
        "doctrine/data-fixtures"               : "~1.0|~2.0",
        "doctrine/migrations"                  : "~1.0|~2.0",
        "doctrine/doctrine-migrations-bundle"  : "~1.0",
        "sensiolabs/security-checker"          : "~3.0",
        "scrutinizer/ocular"                   : "~1.1",
        "satooshi/php-coveralls"               : "~0.6",
        "sami/sami"                            : "~3.0",
        "nikic/php-parser"                     : ">=1.0,<1.2"
    },
    "suggest" : {
        "symfony/symfony" : ">=2.7"
    },
    "autoload" : {
        "psr-4" : {
            "Scribe\\WonkaBundle\\" : "src/"
        }
    },
    "config" : {
        "bin-dir"           : "bin",
        "preferred-install" : "auto",
        "minimum-stability" : "dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master" : "0.1-dev"
        }
    }
}
